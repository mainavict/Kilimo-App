generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
  url      = "file:./dev.db"
//   provider = "postgresql"
//   url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}